/*===============
article_listNews
===============*/

.listcard_news .box_text {
    padding-left: 30px;
}
.listcard_news .box_text:before {
    content: "";
    position: absolute;
    top: 51%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    left: 15px;
    display: block;
    width: 2px;
    height: 78%;
    background-color: #e3e3e3;
}
@media screen and (max-width: 768px) {
    .listcard_news {
        display: -webkit-flex;
        display: flex;
    }
    .listcard_news .box_img {
        width: 32%;
        height: auto;
    }
    .listcard_news .box_img img {
        position: static;
        -webkit-transform: none;
        transform: none;
        width: 100%;
        height: auto;
    }
    .listcard_news .box_text {
        width: 68%;
        padding: 0 14px;
    }
    .listcard_news .box_text .day {
        font-size: 9px;
        font-style: italic;
        color: #969696;
    }
    .listcard_news .box_text h2 {
        margin-bottom: 10px;
        line-height: 1.41;
    }
    .listcard_news .box_text:before {
        width: 1px;
        left: 9px;
        height: 100%;
    }
}

/*===============
category
===============*/

.list_category li:not(:last-child) {
    position: relative;
    margin-right: 5px;
}
.list_category li:not(:last-child):after {
    content: ",";
    display: inline;
    color: #c6a76b;
}
[class*="interview"] .list_category li:not(:last-child) {
    position: relative;
    margin-right: 20px;
    font-weight: lighter;
}
[class*="interview"] .list_category li:not(:last-child):after {
    content: "";
    display: block;
    width: 1px;
    position: absolute;
    height: 70%;
    top: 50%;
    right: -11px;
    -webkit-transform: translateY(-65%);
    transform: translateY(-65%);
    background: #c0c0c0;
}

/*==================================================
機能の共通設定
==================================================*/


/*=========================
font-weght
=========================*/

.bold {
    font-weight: bold !important;
}
.bold_pc {
    font-weight: bold !important;
}
.bold_sp {
    font-weight: normal !important;
}

/*=========================
 placeholder
=========================*/

input:focus::placeholder,
textarea:focus::placeholder {
    color: transparent;
}

/*=========================
 インライン要素の配置用box
=========================*/

.box_inlineCenter {
    position: relative;
    display: -webkit-flex !important;
    display: flex !important;
    -webkit-justify-content: center;
    justify-content: center;
    width: 100%;
    text-align: center;
}

/*=========================
 class「sp」のものは、
 スマホの時のみ表示
=========================*/

.sp {
    display: none !important;
}

/*=========================
 子要素をfloatさせる際に、
 親要素に指定、高さを保持する
=========================*/

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    font-size: 0;
    clear: both;
    visibility: hidden;
}

/*=========================
 改行の無効
=========================*/

.nowrap {
    white-space: nowrap;
}

/*=========================
 画像の最大値
=========================*/

img {
    max-width: 100%;
    height: auto;
}

/*=========================
 アイコンの表示
=========================*/

[class*="icon"] {
    position: relative;
    display: block;
    background-repeat: no-repeat;
}
[class*="icon"]:after,
[class*="icon"]:before {
    content: "";
    position: absolute;
    display: inline-block;
    background-repeat: no-repeat;
    background-position: center;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

/*=========================
 平行四辺形矢印
=========================*/

[class*="icon_skewArrow"]:after {
    width: 60px;
    height: 7px;
    border-bottom: 1px solid #c5a76b;
    border-right: 1px solid #c5a76b;
    -webkit-transform: translateY(-50%) skewX(45deg);
    transform: translateY(-50%) skewX(45deg);
    transition: all .5s ease;
    top: 46.5%;
}
.btn[class*="icon_skewArrow"]:after {
    right: 25px;
}
.icon_skewArrowLeft:after {
    -webkit-transform: translateY(-50%) scale(-1, 1) skewX(45deg);
    transform: translateY(-50%) scale(-1, 1) skewX(45deg);
}
.icon_skewArrowTop:after {
    -webkit-transform: translateY(-50%) rotate(-90deg) skewX(45deg);
    transform: translateY(-50%) rotate(-90deg) skewX(45deg);
}
.icon_skewArrowBottom:after {
    -webkit-transform: translateY(-50%) rotate(90deg) skewX(45deg);
    transform: translateY(-50%) rotate(90deg) skewX(45deg);
}
@media screen and (min-width: 769px) {
    [class*="icon_skewArrow"]:hover:after {
        border-bottom: 1px solid #fff;
        border-right: 1px solid #fff;
        right: 15px !important;
    }
    .icon_skewArrowLeft:hover:after {
        border-bottom: 1px solid #fff;
        border-right: 1px solid #fff;
        left: 15px !important;
    }
}
@media screen and (max-width: 768px) {
    [class*="btn"][class*="icon_skewArrow"]:after {
        width: 30px;
        height: 4px;
    }
}

/*=========================
別タブアイコン
=========================*/

.icon_tabOpen span,
.icon_backTo span {
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    display: inline-block;
    transition: all 0.5s ease;
}
.icon_tabOpen span {
    width: 25px;
    height: 24px;
    right: 32px;
    border: 1px solid #c5a76b;
    border-right: none;
}
.icon_tabOpen.icon_skewArrow:after {
    width: 25px;
    height: 5px;
    top: 49%;
    right: 22px;
}
.icon_tabOpen.icon_skewArrow:hover:after {
    width: 35px;
    right: 12px;
}
.icon_tabOpen span:after,
.icon_tabOpen span:before {
    right: 0;
    width: 1px;
    height: 5px;
    background-color: #c5a76b;
    -webkit-transform: none;
    transform: none;
    transition: all 0.5s ease;
}
.icon_tabOpen span:after {
    top: 0;
}
.icon_tabOpen span:before {
    top: auto;
    bottom: 0;
}
@media screen and (min-width: 769px) {
    .icon_tabOpen:hover span {
        border: 1px solid #fff;
        border-right: none;
    }
    .icon_tabOpen:hover span:after,
    .icon_tabOpen:hover span:before {
        background-color: #fff;
    }
}
@media screen and (max-width: 768px) {
    .icon_tabOpen span {
        width: 22px;
        height: 21px;
    }
    .icon_tabOpen.icon_skewArrow:after {
        width: 21px;
        height: 5px;
        top: 46%;
    }
}

/*=========================
ページバックアイコン
=========================*/

.icon_backTo span {
    width: 30px;
    height: 30px;
    right: 32px;
    border-right: 1px solid #c5a76b;
    border-bottom: 1px solid #c5a76b;
    border-radius: 5px;
}
.icon_backTo.icon_skewArrow:after {
    right: 33px;
    top: 50px;
}

/*=========================
 リンクボタン　画像ボタンの設定
=========================*/

a {
    transition: all .5s ease;
    cursor: pointer;
    text-decoration: none;
    color: #48483f;
}
.btn_top {
    text-align: center;
    padding: 37px 0 10px;
    background: #42a58d;
    color: #fff;
}
.btn_top.icon_lineArrowTop:before {
    -webkit-transform: translate(-50%) rotate(90deg);
    transform: translate(-50%) rotate(90deg);
    left: 50%;
    top: 19px;
}
.btn_top.icon_lineArrowTop:after {
    width: 9px;
    height: 9px;
    -webkit-transform: translate(-50%, -50%) rotate(-45deg);
    transform: translate(-50%, -50%) rotate(-45deg);
    top: 19px;
    left: 50%;
}

/*=========================
 ボタン
=========================*/

[class*="btn"],
[class*="btn"]+label {
    position: relative;
    display: inline-block;
    width: 540px;
    padding: 30px 0;
    border: 1px solid #c5a76b;
    text-align: center;
    cursor: pointer;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 1;
    font-size: 18px;
    color: #c5a76b;
    text-decoration: none;
    transition: all .5s ease;
    z-index: 1;
    letter-spacing: 0.075em;
}
@media screen and (min-width: 769px) {
    [class*="btn"]:before,
    [class*="btn"]+label:before {
        content: "";
        display: inline-block;
        width: 100%;
        height: 100%;
        top: 0;
        left: -100%;
        background: #c5a76b;
        position: absolute;
        transition: all .5s ease;
        -webkit-transform: none;
        transform: none;
        z-index: -11;
    }
    [class*="btn"]:hover:before,
    [class*="btn"]+label:hover:before {
        left: 0;
    }
    [class*="btn"]:hover,
    [class*="btn"]+label:hover {
        color: #fff;
    }
}
@media screen and (max-width: 768px) {
    [class*="btn"],
    [class*="btn"]+label {
        width: 100%;
        padding: 18px 0;
        font-size: 14px !important;
    }
}
@media screen and (max-width: 320px) {
    [class*="btn"],
    [class*="btn"]+label {
        font-size: 12px !important;
    }
}

/* ====================================================================================================
/////SP
==================================================================================================== */

@media screen and (max-width: 768px) {
    /*==================================================
    コンテンツエリアの共通設定
    ==================================================*/
    /*=========================
    body
    =========================*/
    body {
        font-size: 14px;
        overflow: visible;
    }
    /*==================================================
    機能の共通設定
    ==================================================*/
    /*=========================
    class「pc」のものは、PCの時のみ表示
    =========================*/
    .pc {
        display: none !important;
    }
    .sp {
        display: block !important;
    }
    /*=========================
    font-weght
    =========================*/
    .bold_pc {
        font-weight: normal !important;
    }
    .bold_sp {
        font-weight: bold !important;
    }
    /*=========================
    overlay
    =========================*/
    #overlay {
        display: none;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 2;
        background: #000;
        opacity: 0.5;
    }
    .scrollFixed {
        position: fixed;
        width: 100%;
        height: 100%;
    }
}
.target>* {
    position: relative !important;
    top: 50px !important;
    opacity: 0 !important;
    transition: 0.6s all ease !important;
}
.child {
    top: 0 !important;
    opacity: 1 !important;
}
