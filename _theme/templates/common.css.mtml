@charset "UTF-8";

/* CSS Document */


/*==================================================
ヘッダー、フッターの設定
==================================================*/


/*=========================
WEB font
=========================*/

@font-face {
    font-family: "NotoSansCJKjp";
    font-weight: 200;
    src: url("../font/NotoSansCJKjp-Light.woff") format("woff");
}
@font-face {
    font-family: "NotoSansCJKjp";
    font-weight: 300;
    src: url("../font/NotoSansCJKjp-DemiLight.woff") format("woff");
}
@font-face {
    font-family: "NotoSansCJKjp";
    font-weight: 400;
    src: url("../font/NotoSansCJKjp-Regular.woff") format("woff");
}
@font-face {
    font-family: "NotoSansCJKjp";
    font-weight: 500;
    src: url("../font/NotoSansCJKjp-Medium.woff") format("woff");
}
@font-face {
    font-family: "NotoSansCJKjp";
    font-weight: bold;
    src: url("../font/NotoSansCJKjp-Bold.woff") format("woff");
}

/*=========================
body
=========================*/

* {
    box-sizing: border-box;
}
body {
    color: #252525;
    font-size: 14px;
    font-family: "Poppins", "NotoSansCJKjp", -apple-system, "BlinkMacSystemFont", Sans-Serif, "ヒラギノ角ゴシック Pro", "Hiragino Kaku Gothic Pro", メイリオ, Meiryo, Osaka, "ＭＳ Ｐゴシック", "MS PGothic", sans-serif !important;
    -ms-word-wrap: break-word;
    word-wrap: break-word;
    -webkit-text-size-adjust: 100%;
    overflow: hidden;
    letter-spacing: 0.025em;
}
main>article {
    padding-top: 148px;
}

/* IE11対策 */

main {
    display: block;
}
@media screen and (max-width: 768px) {
    main>article {
        padding-top: 60px;
    }
}

/*=========================
body > header
=========================*/

body>header {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 90px;
    z-index: 2;
    background-color: rgba(255, 255, 255, 0.9);
}
body>header>* {
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
body>header>.logo {
    width: 118px;
    top: 55%;
    left: 50px;
}

/*=========================
パンくず
=========================*/

#breadcrumb {
    position: absolute;
    width: 100%;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: flex-end;
    justify-content: flex-end;
    top: 105px;
    right: 50px;
}
#breadcrumb>div {
    position: relative;
    border-top: 1px solid #c6a76b;
    font-size: 11px;
    font-style: italic;
    padding-top: 4px;
    letter-spacing: 0.075em;
}
#breadcrumb>div:last-child {
    color: #c6a76b;
}
#breadcrumb>div:first-child:before {
    position: absolute;
    content: "";
    display: block;
    width: 100%;
    height: 1px;
    background-color: #ccc;
    left: -100%;
    top: -1px;
}
@media screen and (max-width: 768px) {
    /*=========================
    body > header
    =========================*/
    body>header {
        height: 55px;
        background-color: #fff !important;
    }
    body>header>.logo {
        width: 55px;
        left: 20px;
    }
    #breadcrumb {
        top: 60px;
    }
}

/*=========================
#gNav
=========================*/

#wrap_gNav {
    width: 515px;
    right: 50px;
}
#gNav {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
}
#gNav a {
    display: inline;
    font-size: 15px;
    letter-spacing: 0.1em;
}
#gNav li.current_page_item>a {
    color: #c6a76b;
}
#gNav li.current_page_item>a span:nth-child(3) {
    color: #48483f;
}
#gNav li .icon_underbar {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: block;
}
@media screen and (min-width: 769px) {
    #gNav li {
        position: relative;
        padding: 5px 0;
        ;
    }
    #gNav li a {
        position: relative;
        z-index: 3;
    }
    #gNav li .icon_underbar {
        top: 50%;
        -webkit-transform: translateY(-50%);
        transform: translateY(-50%);
        overflow: hidden;
        height: calc(100% + 6px);
        transition: all .5s ease;
    }
    #gNav li .icon_underbar:before {
        top: auto;
        bottom: 1px;
        left: -100%;
        display: inline-block;
        width: 100%;
        height: 1px;
        background-color: #c6a76b;
        transition: all .5s ease;
        -webkit-transform: none;
        transform: none;
    }
    #gNav a:hover {
        color: #c6a76b !important;
    }
    #gNav li a:hover+.icon_underbar:before {
        left: 0;
    }
    #gNav li:last-child {
        /*display: none;*/
    }
}
@media screen and (max-width: 768px) {
    #wrap_gNav {
        display: none;
        position: absolute;
        top: 55px;
        left: 0;
        -webkit-transform: none;
        transform: none;
        width: 100%;
        text-align: center;
        background: #fff;
        z-index: 1;
    }
    html,
    body {
        height: 100%;
    }
    #wrap_gNav ul {
        display: block;
        background-color: #e1ca9c;
        height: 100%;
        overflow: scroll;
        -webkit-overflow-scrolling: touch
    }
    #wrap_gNav li a {
        display: inline-block;
        width: 100%;
        padding: 23px 0 22px 55px;
        color: #fff;
        font-size: 18px;
        font-weight: normal;
        text-align: left;
    }
    #wrap_gNav li:first-child a {
        padding-top: 50px;
    }
    #wrap_gNav li:nth-last-child(2) {
        position: relative;
    }
    #wrap_gNav li:nth-last-child(2) a {
        font-size: 12px;
    }
    #wrap_gNav li:nth-last-child(2) a {
        font-size: 12px;
    }
    #wrap_gNav li:nth-last-child(2) a.sns_fb_follow {
        position: absolute;
        padding-right: 0;
        display: inline-block;
        width: auto;
        right: 20px;
        top: 50%;
        -webkit-transform: translateY(-50%);
        transform: translateY(-50%);
        text-align: right;
    }
    #wrap_gNav li:nth-last-child(2) a.sns_fb_follow .icon_facebook:after {
        right: 5px;
        left: auto;
        top: -8px;
    }
    #wrap_gNav li:nth-last-child(2) a.sns_fb_follow {}
    #gNav li.current_page_item>a.sns_fb_follow {
        color: #fff;
    }
    #gNav .btn_call {
        width: 90%;
        border-color: #fff;
        color: #fff;
        font-size: 16px !important;
        margin: 37px 0 30px;
        padding: 15px 0;
        text-align: center;
    }
    #gNav .btn_call.icon_skewArrow:after {
        right: 20px;
        border-color: #fff;
        -webkit-transform: translateY(-50%) rotate(-45deg) skewX(45deg);
        transform: translateY(-50%) rotate(-45deg) skewX(45deg);
    }
}

/*=========================
#toggle
=========================*/

[class*="toggle"] {
    position: absolute;
    height: 20%;
    width: 20px
}
[class*="toggle"] span {
    display: inline-block;
    transition: all 0.4s;
    position: absolute;
    top: 50%;
    width: 100%;
    height: 2px;
    background-color: #48483f;
}
[class*="toggle"] span.top {
    top: 0;
}
[class*="toggle"] span.middle {
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
[class*="toggle"] span.bottom {
    top: auto;
    bottom: 0;
}
[class*="toggle"].active span.top {
    top: 40%;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
[class*="toggle"].active span.middle {
    opacity: 0;
}
[class*="toggle"].active span.bottom {
    bottom: 42%;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
}
header .toggle {
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    right: 20px;
}

/*=========================
footer
=========================*/

footer {
    position: relative;
    width: 91.5%;
    height: 305px;
    margin: 0 auto 70px;
    color: #fff;
    font-size: 12px;
    line-height: 1.65;
    letter-spacing: 0.015em;
    background: #333;
}
footer p {
    color: #a69f8f;
}
footer>.block {
    position: relative;
    padding: 50px 0 25px;
}
footer>div {
    position: absolute;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    color: #a69f8f;
}
footer>div.left {
    left: 80px;
}
footer>div.left>span {
    display: block;
    width: 108px;
    margin-bottom: 25px;
}
footer>div.left p:not(:last-child) {
    margin-bottom: 11px;
}
footer>div.right {
    width: 510px;
    right: 80px;
}
footer>div.right ul {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between
}
footer>div.right li a {
    color: #fff;
    font-size: 14px;
    letter-spacing: 0.1em;
    transition: all .5s ease;
}
#fNav li.current_page_item>a {
    color: #aea89a;
}
footer>div.right li:last-child {
    position: absolute;
    right: 0;
    top: 25px;
}
footer>div.right li:last-child a {
    color: #a69f8f;
    font-size: 11px;
}
.sns_fb_follow {
    display: block;
    width: 100%;
    text-align: right;
    margin-top: 90px;
    color: #fff;
}
.sns_fb_follow .icon_facebook {
    position: relative;
    padding-right: 20px;
}
.sns_fb_follow .icon_facebook:after {
    width: 9px;
    height: 18px;
    top: auto;
    right: 0;
    bottom: 4px;
    -webkit-transform: none;
    transform: none;
    background-image: url(<$mt:BlogRelativeURL$>common/images/sns_fb_follow.png);
    background-size: 100%;
    transition: all .5s ease;
}
#copyright {
    position: absolute;
    bottom: -45px;
    left: 0;
    width: 100%;
    color: #b5ae9a;
    font-size: 10px;
    letter-spacing: 0.08em;
    text-align: center;
    background: #fff;
}
@media screen and (min-width: 769px) {
    footer>div.right li a:hover {
        color: #aea89a;
    }
    footer>div.right li:last-child a:hover {
        color: #958c78;
    }
    .sns_fb_follow .icon_facebook:hover:after {
        background-image: url(<$mt:BlogRelativeURL$>common/images/sns_fb_follow_on.png);
    }
}
@media screen and (max-width: 768px) {
    footer {
        width: 90%;
        height: auto;
        padding: 45px 16px;
    }
    footer p {
        font-size: 13px;
    }
    footer>div {
        position: static;
        -webkit-transform: none;
        transform: none;
    }
    footer>div.left {
        margin-bottom: 20px;
    }
    footer>div.left>span {
        width: 53px;
        margin-bottom: 3px;
    }
    footer>div.left p:not(:last-child) {
        margin-bottom: 3px;
    }
    footer>div.right {
        width: 100%;
    }
    footer>div.right nav {
        position: relative;
        border-top: 1px solid #c0b9a7;
        border-bottom: 1px solid #c0b9a7;
        margin-bottom: 20px;
        padding: 16px 0 36px;
    }
    footer>div.right ul {
        -webkit-flex-wrap: wrap;
        flex-wrap: wrap;
    }
    footer>div.right li {
        width: calc(100% / 3);
        line-height: 2.3;
    }
    footer>div.right li a {
        font-size: 12px;
    }
    footer>div.right li:nth-child(3n-2) {
        text-align: left;
    }
    footer>div.right li:nth-child(3n-1) {
        text-align: center;
    }
    footer>div.right li:nth-child(3n) {
        text-align: right
    }
    footer>div.right li:last-child {
        width: auto;
        top: auto;
        left: 0;
        bottom: 10px;
    }
    footer>div.right li:last-child a {
        font-size: 8px;
    }
    .sns_fb_follow {
        margin-top: 0;
        text-align: left;
    }
    .sns_fb_follow span {
        font-size: 9px;
    }
    .sns_fb_follow .icon_facebook:after {
        left: 50px;
    }
}
@media screen and (max-width: 768px) {
    footer {
        padding: 33px 16px;
    }
    footer p {
        font-size: 9px;
    }
    footer>div.right li a {
        font-size: 10px;
    }
}

/*=========================
aside_sns
=========================*/

.aside_sns {
    padding: 80px 0;
    text-align: center;
}
.aside_sns img {
    width: 50%;
}
.aside_sns .block p {
    margin-bottom: 18px;
    font-size: 14px;
    letter-spacing: 0.07em;
}
.aside_sns .block>div {
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: center;
    justify-content: center;
}
.sns_fb_share {
    margin-right: 20px;
}
@media screen and (min-width: 769px) {
    .aside_sns .block>div a:hover {
        opacity: 0.7;
    }
}
@media screen and (max-width: 768px) {
    .aside_sns {
        padding: 40px 0;
    }
    .aside_sns .block p {
        font-size: 10px;
    }
}

/*==================================================
メインコンテンツエリアの共通設定
==================================================*/


/*=========================
タイトル
=========================*/


/* ttl_page */

.ttl_page {
    margin-bottom: 40px;
}
.ttl_page h1 {
    margin-bottom: 40px;
    font-size: 48px;
    font-weight: 500;
    font-style: italic;
    letter-spacing: 0.075em;
}
.ttl_page h1 span {
    position: relative;
    left: 15px;
    color: #707070;
    font-size: 16px;
    font-style: initial;
    font-weight: 400;
    letter-spacing: 0.025em;
}
.ttl_page .text_concept {
    margin-bottom: 22px;
    color: #c6a76b;
    font-size: 40px;
    font-style: italic;
    line-height: 1.25;
    letter-spacing: 0.075em;
}
.ttl_page p:not(.text_concept) {
    font-size: 18px;
    width: 82%;
    line-height: 2;
    letter-spacing: 0.025em;
}
[class*='article_detail'] .ttl_page h1 {
    margin-bottom: 7px;
    font-size: 40px;
    font-style: inherit;
    line-height: 1.45;
}

/* ttl_article */

.ttl_article {
    margin-bottom: 45px;
}
.ttl_article h2 {
    font-size: 30px;
    font-weight: 400;
    letter-spacing: 0.075em;
}
.ttl_article h2 span {
    position: relative;
    left: 15px;
    font-size: 14px;
    letter-spacing: 0.025em;
    color: #707070;
}
@media screen and (max-width: 768px) {
    .ttl_page {
        margin-bottom: 32px;
    }
    /* ttl_page */
    .ttl_page h1 {
        margin-bottom: 23px;
        font-size: 27px;
    }
    .ttl_page h1 span {
        display: block;
        left: 0;
        margin-top: 4px;
        font-size: 12px;
        line-height: 1.5;
    }
    .ttl_page .text_concept {
        font-size: 24px;
        line-height: 1.16;
    }
    .ttl_page p:not(.text_concept) {
        font-size: 14px;
        width: 100%;
        line-height: 1.64;
    }
    /* ttl_article */
    .ttl_article {
        margin-bottom: 20px;
    }
    .ttl_article h2 {
        font-size: 19px;
    }
    .ttl_article h2 span {
        display: block;
        left: 0;
        margin-top: 6px;
        font-size: 12px;
        line-height: 1.54;
    }
}

/*=========================
article block
=========================*/

[class*="block"] {
    margin: 0 auto;
    max-width: 1080px;
}
.article .block_section {
    max-width: 900px;
}
@media screen and (max-width: 768px) {
    .block:not(.block_section),
    .article .block_section {
        width: 86%;
        max-width: none;
    }
}

/*=========================
article_intro
=========================*/

.moreread {
    position: absolute !important;
    z-index: -2;
    visibility: hidden;
}
.article_intro {
    padding: 35px 0px 80px;
}
@media screen and (max-width: 768px) {
    .article_intro {
        padding: 26px 0px 40px;
    }
}

/*=========================
article_information
=========================*/

[class*="article_information"].hidden {
    display: none !important;
}
[class*="article_information"] {
    padding: 75px 0 0;
}
[class*="article_information"] .block div {
    margin-top: 32px;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}
[class*="article_information"] .block dl {
    width: 33.3%;
    line-height: 1.75;
    text-align: left;
}
[class*="article_information"] .block dl:not(:nth-last-child(-n+3)) {
    margin-bottom: 30px;
}
[class*="article_information"] .block dd {
    color: #707070;
    font-size: 15px;
}
[class*="article_information"] .block div.box_inlineCenter {
    margin-top: 40px;
}
@media screen and (max-width: 768px) {
    [class*="article_information"] {
        padding: 38px 0 0;
    }
    [class*="article_information"] .block div {
        display: block;
        margin-top: 23px;
    }
    [class*="article_information"] .block dl {
        width: 100%;
        line-height: 1.5;
    }
    [class*="article_information"] .block dl dt {
        font-size: 11px;
    }
    [class*="article_information"] .block dl dd {
        font-size: 12px;
    }
    [class*="article_information"] .block dl:not(:last-child) {
        margin-bottom: 18px;
    }
}

/*=========================
article_list 共通設定
=========================*/

[class*="article_list"] {
    padding: 50px 0;
}
[class*="article_list"] .ttl_article {
    margin-bottom: 27px;
}
[class*="article_list"] .block>ul {
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}
[class*="article_list"] .box_inlineCenter {
    margin-top: 25px;
}
[class*="listcard"] {
    position: relative;
    width: calc((100% / 3) - 20px);
    margin-bottom: 50px;
    box-sizing: border-box;
    /* opacity: 0; */
    background-color: #fff;
}
[class*="listcard"]:not(:nth-child(3n)) {
    margin-right: 30px;
}
[class*="listcard"] .box_img {
    position: relative;
    height: 235px;
    overflow: hidden;
    background-color: #fff;
}
[class*="listcard"] .box_img img {
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    transition: all 0.5s ease-out;
    width: auto;
    max-width: none;
    height: 110%;
}
[class*="listcard"]>a {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    display: block;
}
@media screen and (min-width: 769px) {
    [class*="listcard"]:hover .box_img img {
        height: 120%;
    }
    [class*="listcard"]>a:hover {
        background-color: rgba(255, 255, 255, 0.3);
    }
}
[class*="listcard"] .box_text {
    position: relative;
    padding: 20px 30px;
}
[class*="listcard"] .box_text h2 {
    margin-bottom: 25px;
    color: #252525;
    font-size: 16px;
    line-height: 1.62;
    font-weight: 400;
}
[class*="listcard"] .list_category {
    z-index: 1;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
    font-size: 12px;
    font-style: italic;
    font-weight: lighter;
    letter-spacing: 0.1em;
    line-height: 1.3;
}
[class*="listcard"] .list_category li a {
    color: #c6a76b;
}
@media screen and (max-width: 768px) {
    [class*="article_list"] {
        padding: 40px 0;
    }
    [class*="listcard"] .box_text {
        padding: 12px 13px;
    }
    [class*="listcard"] .box_text h2 {
        font-size: 14px;
    }
    [class*="article_list"] .ttl_article {
        margin-bottom: 17px;
    }
    [class*="listcard"] {
        width: 100%;
        margin-bottom: 25px;
    }
    [class*="listcard"]:not(:nth-child(3n)) {
        margin-right: 0;
    }
    [class*="listcard"] .list_category {
        font-size: 9px;
    }
    [class*="listcard"] .box_img {
        height: 62.93vw;
    }
}
@media screen and (max-width: 320px) {
    [class*="listcard"] .box_text h2 {
        font-size: 12px;
    }
}

/*===============
article_listinterview
===============*/

.article_listInterview {
    background-color: #f9f5e7;
}
.day {
    margin-top: 8px;
    font-size: 12px;
    font-style: italic;
    color: #969696;
}
.listcard_interview .box_text {
    height: 175px;
    background-color: #fff;
}
